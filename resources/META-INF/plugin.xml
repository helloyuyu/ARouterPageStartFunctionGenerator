<idea-plugin>
  <id>com.helloyuyu.plugin.arouternavigatefunctiongenerator</id>
  <name>ARouter Navigate Function Generator</name>
  <version>1.0.1</version>
  <vendor email="jsxie1024@163.com" >holloyuyu</vendor>

  <description><![CDATA[
      Plugin to generate start activity functions.
      <br>
      <em></em>
    ]]></description>

  <change-notes><![CDATA[
      <br>
      <em></em>
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->
  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
    <action id="GenerateARouterNavigateFunctionAction"
            class="com.helloyuyu.plugin.arouternavigatefunctiongenerator.NavigationBuildFunctionAction"
            text="GenerateNavigateFunction" description="Generate ARouter navigate function">
      <add-to-group group-id="GenerateGroup" anchor="last"/>
    </action>
  </actions>

</idea-plugin>